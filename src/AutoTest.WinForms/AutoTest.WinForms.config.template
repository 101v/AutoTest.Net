    <DirectoryToWatch>C:\Users\sveina\src\DotNET\Work\MBS\Main\Synchronization Service\JobExchangeClient</DirectoryToWatch>
    
    <!-- Build executables. Supported are MSBuild.exe and Mono's xbuild -->
